# Server configuration
PORT=8080  # GCP Cloud Run standard port
NODE_ENV=production

# Database
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<dbname>?retryWrites=true&w=majority

# Authentication
JWT_SECRET=your_jwt_secret_key_here

# Email configuration (Nodemailer fallback)
EMAIL_SERVICE=gmail
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-app-specific-password
ADMIN_EMAIL=admin@yourdomain.com
SUPPORT_EMAIL=support@yourdomain.com

# EmailJS configuration
EMAILJS_SERVICE_ID=your_emailjs_service_id
EMAILJS_PUBLIC_KEY=your_emailjs_public_key
EMAILJS_PRIVATE_KEY=your_emailjs_private_key
EMAILJS_ADMIN_TEMPLATE_ID=template_id_for_admin_notifications
EMAILJS_USER_TEMPLATE_ID=template_id_for_user_notifications

# URLs
FRONTEND_URL=https://your-frontend-url.vercel.app
BASE_URL=https://your-backend-url.run.app

# CORS configuration
ALLOWED_ORIGINS=https://your-frontend-url.vercel.app,https://your-preview-url.vercel.app



